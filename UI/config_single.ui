<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="Frame">
    <property name="cursor">arrow</property>
    <property name="height">200</property>
    <property name="padding">18</property>
    <property name="relief">groove</property>
    <property name="takefocus">true</property>
    <property name="width">150</property>
    <layout>
      <property name="column">0</property>
      <property name="columnspan">1</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="rowspan">1</property>
      <rows>
        <row id="0">
          <property name="pad">0</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="ttk.Button" id="OK_Button">
        <property name="command">ok_event</property>
        <property name="text" translatable="yes">Ok</property>
        <property name="width">9</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="ipadx">0</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Cancel_Button">
        <property name="command">cancel_event</property>
        <property name="text" translatable="yes">Cancel</property>
        <property name="width">7</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="text" translatable="yes">Number of rows:</property>
        <property name="width">0</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">0</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="n">
        <property name="text" translatable="yes">30</property>
        <property name="textvariable">int:n</property>
        <layout>
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="text" translatable="yes">Number of columns:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="m">
        <property name="text" translatable="yes">30</property>
        <property name="textvariable">int:m</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_3">
        <property name="text" translatable="yes">Number of initial cells:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="c">
        <property name="text" translatable="yes">200</property>
        <property name="textvariable">int:initCells</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_4">
        <property name="text" translatable="yes">Update time (100ms above):</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="t">
        <property name="text" translatable="yes">100</property>
        <property name="textvariable">int:Updatetime</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_5">
        <property name="text" translatable="yes">Display cell size (pixel):</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="d">
        <property name="text" translatable="yes">10</property>
        <property name="textvariable">int:cellSize</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
